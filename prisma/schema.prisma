datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Quote {
  id        Int     @id @default(autoincrement())
  symbol    String
  price     Float
  timestamp DateTime @default(now())
}
